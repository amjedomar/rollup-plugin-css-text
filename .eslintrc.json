{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "no-restricted-syntax": "off",
    "no-useless-constructor": "off",
    "no-await-in-loop": "off",
    "prefer-template": "off",
    "no-empty-function": "off"
  }
}
